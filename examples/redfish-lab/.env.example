# ISO download settings
ISO_HTTP_PORT=8080
TARGET_ISO_FILENAME=alpine-standard-3.20.2-x86_64.iso
TARGET_ISO_URL=https://dl-cdn.alpinelinux.org/alpine/v3.20/releases/x86_64/alpine-standard-3.20.2-x86_64.iso

# Redfish provisioning targets (space separated)
TARGET_REDFISH_NODES="redfish-client-1 redfish-client-2"

# Optional: override Redfish credentials for all targets
# TARGET_REDFISH_USERNAME=admin
# TARGET_REDFISH_PASSWORD=password

# Optional: override Redfish port (default 8443)
# TARGET_REDFISH_PORT=8443
